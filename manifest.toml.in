# Name of the speller package in various languages.
# The default is English + autonym:
[speller.name]
en = "@SPELLER_NAME_ENG@"
qda = "@SPELLER_NAME_NATIVE@"

# Description of the speller package in various languages.
# The default is English + autonym:
[speller.description]
en = "@SPELLER_DESC_ENG@"
qda = "@SPELLER_DESC_NATIVE@"

[macos]
system_pkg_id = "no.divvun.MacDivvun.xin-x-qda"

[windows]
system_product_code = "{E5DE46A1-AD85-46CF-959B-694CCF219BD4}_is1"

[package.speller]
name = "@GLANGUAGE@"
